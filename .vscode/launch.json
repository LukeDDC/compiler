      "projectName": "compiler"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-Tool<compiler>",
      "request": "launch",
      "mainClass": "org.antlr.v4.Tool",
      "projectName": "compiler"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-TestRig<compiler>",
      "request": "launch",
      "mainClass": "org.antlr.v4.gui.TestRig",
      "projectName": "compiler"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-FlexibleHashMap<compiler>",
      "request": "launch",
      "mainClass": "org.antlr.v4.runtime.misc.FlexibleHashMap",
      "projectName": "compiler"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-LogManager<compiler>",
      "request": "launch",
      "mainClass": "org.antlr.v4.runtime.misc.LogManager",
      "projectName": "compiler"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-TestRig<compiler>(1)",
      "request": "launch",
      "mainClass": "org.antlr.v4.runtime.misc.TestRig",
      "projectName": "compiler"
    },
    {
      "type": "java",
      "name": "Debug (Launch)-STViz<compiler>",
      "request": "launch",
      "mainClass": "org.stringtemplate.v4.gui.STViz",
      "projectName": "compiler"
    },
    {
      "name": "run debug",
      "type": "antlr-debug",
      "request": "launch",
      "input": "teste.rb",
      "grammar": "RUBY.g4",
      "startRule": "program",
      "printParseTree": true,
      "visualParseTree": true
    },

    {
      "name": "debug2",
      "type": "antlr-debug",
      "request": "launch",
      "grammar": "RUBY.g4",
      "startRule": "program",
      "printParseTree": true,
      "visualParseTree": true
